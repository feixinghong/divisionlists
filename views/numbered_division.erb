<?xml version="1.0" encoding="UTF-8"?>
<division>
  <number><%= @division["number"] %></number>
  <date><%= @division["date"] %></date>
  <file><%= @division["file"] %></file>
  <page><%= @division["page"] %></page>
  <resolution><%= @division["resolution"] %></resolution>
  <ayes><% @division["ayes"].each do |member|%>
    <member><%= member %></member><%end%>
  </ayes>
  <ayes_tellers><%= @division["ayes_tellers"] %></ayes_tellers>
  <noes><% @division["noes"].each do |member|%>
    <member><%= member %></member><%end%>
  </noes>
  <noes_tellers><%= @division["noes_tellers"] %></noes_tellers>
  <debug><%= @division.inspect %></debug>
</division>